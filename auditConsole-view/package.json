{
  "name": "audit-console-view",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json --host 0.0.0.0",
    "build": "ng build --prod --aot --base-href \"/auditConsole/\"",
    "build-source": "ng build --prod --aot --source-map=true --base-href \"/auditConsole/\"",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:mock:server": "npm run concat:mocks && json-server --routes src/mocks/routes.json --watch src/mocks/data.json",
    "start:proxy:mock": "ng serve --proxy-config proxy.mock.json --host 0.0.0.0 --aot",
    "start:proxy:mock:server": "concurrently --kill-others \"npm run start:mock:server\" \"npm run start:proxy:mock\"",
    "concat:mocks": "node \"./src/mocks/concat-json.js\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.10",
    "@angular/common": "^6.1.10",
    "@angular/compiler": "^6.1.10",
    "@angular/core": "^6.1.10",
    "@angular/forms": "^6.1.10",
    "@angular/http": "^6.1.10",
    "@angular/platform-browser": "^6.1.10",
    "@angular/platform-browser-dynamic": "^6.1.10",
    "@angular/router": "^6.1.10",
    "@fortawesome/angular-fontawesome": "^0.1.1",
    "@fortawesome/fontawesome-free": "^5.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/free-brands-svg-icons": "^5.5.0",
    "@fortawesome/free-regular-svg-icons": "^5.5.0",
    "@fortawesome/free-solid-svg-icons": "^5.5.0",
    "@ng-bootstrap/ng-bootstrap": "^2.2.2",
    "@ng-select/ng-select": "^2.12.0",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "@tredi/utils": "^0.0.1",
    "@tredi/widgets-bs": "^0.0.1",
    "@tredi/widgets-tredi": "^0.0.1",
    "bootstrap": "^4.1.3",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.7",
    "font-awesome": "^4.7.0",
    "ngx-cookie-service": "^1.0.10",
    "rxjs": "^6.3.3",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~17.3.6",
    "@angular/cli": "~6.0.8",
    "@angular/compiler-cli": "^6.1.10",
    "@angular/language-service": "^6.1.10",
    "@types/jasmine": "^2.8.11",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "concurrently": "^3.6.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "json-concat": "0.0.1",
    "json-server": "^0.14.0",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
